{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "headers": {
        "Content-Type": "application/javascript"
      },
      "dest": "/_next/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}